#bbcLogo	style.css	/^#bbcLogo {$/;"	i
#headerWhiteBar	style.css	/^#headerWhiteBar {$/;"	i
.whiteBarText	style.css	/^.whiteBarText {$/;"	c
bbcLogo	index.html	/^                <img id="bbcLogo" src="images\/bbcLogo.png" alt="bbc logo">$/;"	o
body	style.css	/^body {$/;"	t
header	index.html	/^        <div id="header">$/;"	o
headerRedBar	index.html	/^            <div id="headerRedBar">$/;"	o
headerWhiteBar	index.html	/^            <div id="headerWhiteBar">$/;"	o
